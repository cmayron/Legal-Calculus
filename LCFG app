    if(!c) return;
    copyToClipboard(`${c.fg} ‚Äî ${c.title}: ${c.core}`);
  });
});
$$('button[data-quote]').forEach(btn=>{
  btn.addEventListener('click',()=>{
    const id = Number(btn.getAttribute('data-quote'));
    const c = DATA.find(x=>x.id===id);
    if(!c) return;
    copyToClipboard(`> ${c.fg} ‚Äî ${c.title}\\n> ${c.core}\\n‚Äî LCFG 4.5 Advanced`);
  });
});
$$('button[data-json]').forEach(btn=>{
  btn.addEventListener('click',()=>{
    const id = Number(btn.getAttribute('data-json'));
    const c = DATA.find(x=>x.id===id);
    if(!c) return;
    const blob = new Blob([JSON.stringify(c,null,2)], {type:'application/json'});
    download(blob, `${c.fg.replace(' ','_')}.json`);
  });
});

updateSel();

```

}

function updateSel(){
selCount.textContent = selection.size;
bundleBox.style.display = selection.size ? 'block' : 'none';
}

function buildBundle(){
if(!selection.size){ bundleText.value=''; return; }
const mode = $("#bundleMode").value;
const items = DATA.filter(c=>selection.has([c.id](http://c.id/)))
.sort((a,b)=>[a.id-b.id](http://a.id-b.id/));
let out = '';
if(mode==='raw'){
out = items.map(c=>`${c.fg} ‚Äî ${c.title}\\n‚Ä¢ ${c.core}\\n`).join('\n');
} else if(mode==='hearing'){
out = `HEARING BUNDLE ‚Äî LCFG 4.5\\n\\n` +
items.map(c=>`[${c.fg}] ${c.title}\\nCore: ${c.core}\\nLine: ${c.model||c.core}\\n`).join('\n');
} else if(mode==='motion'){
out = `MOTION BUNDLE ‚Äî LCFG 4.5\\n\\n` +
items.map(c=>`‚Ä¢ ${c.fg} ${c.title}: ${c.core}`).join('\n');
} else { // appeal
out = `APPEAL BUNDLE ‚Äî LCFG 4.5\\n\\n` +
items.map(c=>`${c.fg} ${c.title}\\nArgument: ${c.core}\\n`).join('\n');
}
bundleText.value = out;
}

function copyVisibleLines(){
const lines = current().map(c=>`${c.fg} ‚Äî ${c.title}: ${c.core}`);
copyToClipboard(lines.join('\n'));
}

function exportJson(){
const payload = {
meta: { exportedAt: new Date().toISOString(), count: current().length },
cards: current()
};
const blob = new Blob([JSON.stringify(payload,null,2)], {type:'application/json'});
download(blob, 'LCFG-4.5-Advanced.export.json');
}

function importMdFlow(){
const dlg = document.createElement('div');
dlg.innerHTML =        `<div style="position:fixed;inset:0;background:rgba(0,0,0,.55);display:flex;align-items:center;justify-content:center;z-index:9999">        <div style="width:min(100%,900px);max-height:90vh;overflow:auto;background:var(--surface);border:1px solid var(--border);border-radius:14px;padding:14px">         <h3 style="margin:4px 0 10px 0">Import Markdown (FG 1‚Äì1000)</h3>         <p class="muted">Paste your full Field Guide Markdown. The parser expects sections like <code>### FG-001 ‚Äî Title</code> followed by bullet lines with <b>Core</b>, <b>Lesson</b>, <b>Assignment</b>, <b>Model</b>, <b>Reflection</b>, <b>Visual</b>.</p>         <textarea id="mdArea" style="width:100%;min-height:45vh;background:var(--sunken);color:var(--text);border:1px solid var(--border);border-radius:10px;padding:10px" placeholder="Paste Markdown‚Ä¶"></textarea>         <div class="row" style="margin-top:10px">           <button class="btn btn-primary" id="parseMdBtn">Parse & Replace</button>           <button class="btn" id="appendMdBtn">Parse & Append</button>           <span class="spacer"></span>           <button class="btn btn-danger" id="cancelMdBtn">Cancel</button>         </div>        </div>       </div>`;
document.body.appendChild(dlg);
dlg.querySelector('#cancelMdBtn').onclick = ()=> dlg.remove();
dlg.querySelector('#parseMdBtn').onclick = ()=>{
const text = dlg.querySelector('#mdArea').value;
const parsed = parseMarkdown(text);
if(parsed.length){ DATA = parsed; selection.clear(); render(); }
dlg.remove();
};
dlg.querySelector('#appendMdBtn').onclick = ()=>{
const text = dlg.querySelector('#mdArea').value;
const parsed = parseMarkdown(text);
if(parsed.length){
// merge by FG id (replace duplicates)
const map = new Map(DATA.map(x=>[x.fg,x]));
parsed.forEach(x=>map.set(x.fg,x));
DATA = Array.from(map.values()).sort((a,b)=>[a.id-b.id](http://a.id-b.id/));
selection.clear(); render();
}
dlg.remove();
};
}

function parseMarkdown(md){
if(!md || !md.trim()) return [];
const lines = md.replace(/\r\n/g,"\n").split("\n");
const cards = [];
let cur = null;
const headRe = /^\s*#{2,3}\s*FG-(\d+)\s*‚Äî\s*(.+?)\s*$/i;
const fieldRe = /^\s*-\s*(?:‚öñÔ∏è|üìñ|‚úèÔ∏è|‚úÖ|üí°|üé®)\s*\*\*(.+?)\*\*:\s*(.+)\s*$/;

```
function pushCur(){
  if(!cur) return;
  cur.title = cur.title || cur.rawTitle || '';
  cur.core = cur.core || cur.Core || cur['Core Message'] || cur['Core'] || '';
  cur.model = cur.model || cur.Model || cur['Model Response'] || '';
  cur.type = inferType(cur.title);
  cur.volume = cur.volume || inferVolume(cur.id);
  if(!cur.fg) cur.fg = `FG-${String(cur.id).padStart(3,'0')}`;
  cards.push({
    id: cur.id, fg: cur.fg, volume: cur.volume, type: cur.type, title: cur.title,
    core: cur.core, model: cur.model, tags: cur.tags||[]
  });
}

lines.forEach(line=>{
  const h = headRe.exec(line);
  if(h){
    pushCur();
    cur = { id: Number(h[1]), rawTitle: h[2], title: h[2], tags: [] };
    return;
  }
  if(!cur) return;
  const f = fieldRe.exec(line);
  if(f){
    const key = f[1].trim();
    const val = f[2].trim();
    if(/Core/i.test(key)) cur.core = strip(val);
    if(/Model/i.test(key)) cur.model = strip(val);
    if(/Lesson|Assignment|Reflection|Visual/i.test(key)){ /* ignored in condensed import */ }
    return;
  }
  // tags heuristic
  if(line.trim().startsWith('#')) cur.tags.push(line.trim().slice(1).toLowerCase());
});
pushCur();

return cards.filter(x=>x.id && x.title && x.core);

```

}

function inferVolume(id){
if(id<=50) return 1;
if(id<=100) return 2;
return 3;
}
function inferType(title){
const t = (title||'').toLowerCase();
if(t.includes('filing')) return 'Filing';
if(t.includes('silence')) return 'Silence';
if(t.includes('denial')) return 'Denial';
if(t.includes('default')) return 'Default';
if(t.includes('efficiency')) return 'Efficiency';
if(t.includes('closure')) return 'Closure';
if(t.includes('archive')) return 'Archive';
return 'Card';
}
function strip(s){ return s.replace(/^‚Äú|‚Äù$/g,'').trim(); }

function download(blob, filename){
const a = document.createElement('a');
a.href = URL.createObjectURL(blob);
a.download = filename;
a.click();
URL.revokeObjectURL(a.href);
}
function copyToClipboard(text){
navigator.clipboard?.writeText(text).then(()=>{
toast('Copied');
}).catch(()=>{
// fallback
const ta = document.createElement('textarea');
ta.value = text; document.body.appendChild(ta); ta.select();
try{ document.execCommand('copy'); toast('Copied'); } finally{ ta.remove(); }
});
}
function toast(msg){
const t = document.createElement('div');
t.textContent = msg;
t.style.cssText = 'position:fixed;bottom:18px;right:18px;background:var(--sel);border:1px solid var(--border);padding:8px 10px;border-radius:10px;color:var(--text);z-index:9999';
document.body.appendChild(t);
setTimeout(()=>t.remove(),1100);
}
function escapeHtml(s){ return (s??'').replace(/[&<>"']/g,m=>({ "&":"&","<":"<",">":">","\"":""","'":"'" }[m])); }

function bakeHtml(){
// produce a fresh single-file HTML containing current DATA
const tpl = document.documentElement.outerHTML;
const payload = {
meta:{ name:"LCFG 4.5 Advanced (Baked)", bakedAt:new Date().toISOString(), count: DATA.length },
cards: DATA
};
const baked = tpl.replace(getSeedBlock(), `<script id="seed-json" type="application/json">\\n${JSON.stringify(payload,null,2)}\\n</script>`);
const blob = new Blob([baked], {type:'text/html;charset=utf-8'});
download(blob, 'LCFG-4.5-Advanced.baked.html');
}
function getSeedBlock(){
const seed = $("#seed-json").outerHTML;
return seed.replace(/[.*+?^${}()|[\]\\]/g, '\\$&'); // escape for replace
}

function debounce(fn,ms){
let t; return (...a)=>{ clearTimeout(t); t=setTimeout(()=>fn.apply(this,a),ms); };
}
})();
</script>
</body>
</html>

