<script>
(function(){
const $ = sel => document.querySelector(sel);
const $$ = sel => Array.from(document.querySelectorAll(sel));

let DATA = loadSeed();
let selection = new Set();

const grid = $("#grid");
const q = $("#q");
const volFilter = $("#volFilter");
const typeFilter = $("#typeFilter");
const sortBy = $("#sortBy");
const selCount = $("#selCount");
const countStatus = $("#countStatus");
const bundleBox = $("#bundleBox");
const bundleText = $("#bundleText");

render();

/* Events */
q.addEventListener('input', debounce(render, 120));
volFilter.addEventListener('change', render);
typeFilter.addEventListener('change', render);
sortBy.addEventListener('change', render);
$("#printBtn").addEventListener('click', () => window.print());
$("#clearSelBtn").addEventListener('click', () => { selection.clear(); updateSel(); renderCards(current()); });
$("#bundleBtn").addEventListener('click', buildBundle);
$("#copyBundleBtn").addEventListener('click', () => copyToClipboard(bundleText.value));
$("#copyLinesBtn").addEventListener('click', copyVisibleLines);
$("#exportJsonBtn").addEventListener('click', exportJson);
$("#importMdBtn").addEventListener('click', importMdFlow);
$("#bakeBtn").addEventListener('click', bakeHtml);

function loadSeed(){
try{
const raw = $("#seed-json").textContent.trim();
const parsed = JSON.parse(raw);
return parsed.cards || [];
}catch(e){
console.error(e);
return [];
}
}

function normalize(str){ return (str||"").toLowerCase(); }

function current(){
const term = normalize(q.value);
const vol = volFilter.value;
const typ = typeFilter.value;

```
let list = DATA.slice();

if (term){
  list = list.filter(c=>{
    const hay = `${c.fg} ${c.title} ${c.core} ${(c.tags||[]).join(' ')}`.toLowerCase();
    return term.split(/\\s+/).every(t => hay.includes(t));
  });
}
if (vol) list = list.filter(c=> String(c.volume)===String(vol));
if (typ) list = list.filter(c=> String(c.type)===String(typ));

const [k,dir] = sortBy.value.split('-'); // id|title asc|desc
list.sort((a,b)=>{
  let va = (k==='id'?a.id:a.title||'');
  let vb = (k==='id'?b.id:b.title||'');
  if (typeof va === 'string') va = va.toLowerCase();
  if (typeof vb === 'string') vb = vb.toLowerCase();
  return dir==='asc' ? (va>vb?1:-1) : (va<vb?1:-1);
});

return list;

```

}

function render(){
const list = current();
countStatus.innerHTML = `Showing <b>${list.length}</b> / ${DATA.length} cards`;
renderCards(list);
}

function renderCards(list){
grid.innerHTML = '';
const frag = document.createDocumentFragment();
list.forEach(c=>{
const card = document.createElement('article');
card.className = 'card';
card.innerHTML =          `<div class="sel"><input type="checkbox" ${selection.has(c.id)?'checked':''} data-sel="${c.id}"><span>${c.fg}</span></div>         <h3>${escapeHtml(c.title)} <span class="muted">‚Ä¢ ${c.fg}</span></h3>         <div class="meta">           <span class="tag">Vol ${c.volume}</span>           <span class="tag">${escapeHtml(c.type)}</span>           ${(c.tags||[]).slice(0,4).map(t=>`<span class="tag">#${escapeHtml(t)}</span>`).join('')}         </div>         <div class="core">‚öñÔ∏è ${escapeHtml(c.core)}</div>         ${c.model ?` <div class="muted">‚úÖ Model: ${escapeHtml(c.model)}</div>`:''}         <div class="row">           <button class="btn small" data-copy="${c.id}">Copy line</button>           <button class="btn small" data-quote="${c.id}">Quote</button>           <span class="spacer"></span>           <button class="btn small" data-json="${c.id}">JSON</button>         </div>`       ;
frag.appendChild(card);
});
grid.appendChild(frag);

```
// Wire up checkboxes & buttons
$$('input[data-sel]').forEach(cb=>{
  cb.addEventListener('change', (e)=>{
    const id = Number(e.target.getAttribute('data-sel'));
    e.target.checked ? selection.add(id) : selection.delete(id);
    updateSel();
  });
});
$$('button[data-copy]').forEach(btn=>{
  btn.addEventListener('click',()=>{
    const id = Number(btn.getAttribute('data-copy'));
